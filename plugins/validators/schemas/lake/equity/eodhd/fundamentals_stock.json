{
  "table_name": "fundamentals_stock",
  "description": "주식 종목 펀더멘털 공통 필드 검증 스키마 (EODHD API 기반)",
  "columns": [
    {"name": "General_Code", "type": "String", "nullable": false},
    {"name": "General_Type", "type": "String", "nullable": false},
    {"name": "General_Name", "type": "String", "nullable": false},
    {"name": "General_Exchange", "type": "String", "nullable": true},
    {"name": "General_CurrencyCode", "type": "String", "nullable": true},
    {"name": "General_CountryISO", "type": "String", "nullable": true},
    {"name": "General_Sector", "type": "String", "nullable": true},
    {"name": "General_Industry", "type": "String", "nullable": true}
  ],
  "constraints": {
    "non_nullable": ["General_Code", "General_Name", "General_Type"],
    "patterns": {
      "General_Code": "^[A-Z0-9\\.-]{1,15}$",
      "General_CountryISO": "^[A-Z]{2}$",
      "General_CurrencyCode": "^[A-Z]{3}$"
    }
  }
}
